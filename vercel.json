{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "img/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/register", "dest": "/backend/server.js" },
    { "src": "/api/login", "dest": "/backend/server.js" },
    { "src": "/api/forgot-password", "dest": "/backend/server.js" },
    { "src": "/api/reset-password", "dest": "/backend/server.js" },
    { "src": "/api/my-orders", "dest": "/backend/server.js" },
    { "src": "/get-snap-token", "dest": "/backend/server.js" },
    { "src": "/update-order-status", "dest": "/backend/server.js" },
    { "src": "/api/user/profile", "dest": "/backend/server.js" },
    { "src": "/api/admin/stats", "dest": "/backend/server.js" },
    { "src": "/api/admin/all-orders", "dest": "/backend/server.js" },
    { "src": "/api/admin/update-ticket-status", "dest": "/backend/server.js" },
    { "src": "/api/admin/delete-order/(.*)", "dest": "/backend/server.js" },
    { "src": "/api/admin/reset-user-password", "dest": "/backend/server.js" },
    { "src": "/api/admin/update-cheki-stock", "dest": "/backend/server.js" },
    { "src": "/api/products-and-stock", "dest": "/backend/server.js" },
    { "src": "/api/midtrans-client-key", "dest": "/backend/server.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}